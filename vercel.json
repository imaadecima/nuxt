{
  "version": 2,
  "routes": [
    {
      "src": "/api/test",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/users",
      "dest": "/api/index.js"
    }
  ],
  "builds": [
    {
      "src": "nuxt.config.ts",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": ["api/**"]
      }
    }
  ]
}